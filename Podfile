source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '11.0'

target 'HssRxSwiftProject' do
use_frameworks!

# Networking
pod 'Moya/RxSwift'

# Rx Extensions
pod 'NSObject+Rx'
pod 'RxDataSources'

# JSON Mapping
pod 'Moya-ObjectMapper/RxSwift'

# Tools
pod 'SwiftLint'

# Date
pod 'SwiftDate'

#  UI
pod 'LYEmptyView'
pod 'SVProgressHUD'
pod 'Hero'
pod 'Toast-Swift'

# Auto Layout
pod 'SnapKit'

# Cache Data
pod 'WCDB.swift'

# Logging
pod 'CocoaLumberjack/Swift'

end
